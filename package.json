{
  "name": "datepickerlite",
  "version": "1.0.0",
  "description": "Liteweight datepicker for React",
  "main": "lib/datePickerLite.js",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --config build/webpack.config.js --content-base build --mode=development",
    "build": "webpack --config build/webpack.config.js --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boeriksson/datePickerLite.git"
  },
  "keywords": [
    "datepicker"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boeriksson/datePickerLite/issues"
  },
  "homepage": "https://github.com/boeriksson/datePickerLite#readme",
  "devDependencies": {
    "classnames": "^2.2.5",
    "js-joda": "^1.8.2",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "dirty-chai": "^2.0.1",
    "jest": "^22.4.3",
    "path": "^0.12.7",
    "styled-components": "^3.2.6",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.1.1",
    "webpack-dev-server": "^3.1.3",
    "xhr2": "^0.1.4"
  },
  "peerDependencies": {
    "classnames": "^2.2.5",
    "js-joda": "^1.8.2",
    "lodash": "^4.17.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "<rootDir>/test/jest/shim.js",
      "<rootDir>/test/jest/setup.js"
    ],
    "cacheDirectory": "<rootDir>/test/jest/tmp",
    "coverageDirectory": "<rootDir>/test/jest/coverage",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/jest/_mocks/fileMock.js",
      "\\.(scss)$": "<rootDir>/test/jest/_mocks/dummyFunc.js",
      "postToParent": "<rootDir>/test/jest/_mocks/dummyFunc.js"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
